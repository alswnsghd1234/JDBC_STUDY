<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>query xml</comment>
<entry key="insertProduct">INSERT INTO MEMBER VALUES(SEQ_USERNO.NEXTVAL,?,?,?,?,?,?,?,?,?,DEFAULT)</entry>
<entry key="selectAll">SELECT * FROM MEMBER</entry>
<entry key="selectByProductName">SELECT * FROM MEMBER WHERE USERNAME LIKE '%'||?||'%'</entry>
<entry key="updateProduct">UPDATE MEMBER 
				SET 
				USERPW=?
				,EMAIL=?
				,PHONE=?
				,ADDRESS=? 
				WHERE USERID=?</entry>
<entry key="deleteProduct">DELETE FROM MEMBER WHERE USERID =?</entry>
</properties>
