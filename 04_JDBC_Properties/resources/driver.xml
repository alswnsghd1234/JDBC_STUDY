<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>driver xml</comment>
<entry key="insertMember">INSERT INTO MEMBER VALUES(SEQ_USERNO.NEXTVAL,?,?,?,?,?,?,?,?,?,DEFAULT)</entry>
<entry key="selectAll">SELECT * FROM MEMBER</entry>
<entry key="searchById">SELECT * FROM MEMBER WHERE USERID =?</entry>
<entry key="searchByName">SELECT * FROM MEMBER WHERE USERNAME LIKE '%'||?||'%'</entry>
<entry key="updateMember">UPDATE MEMBER "
				+	 "SET "
				+    "USERPW=?"
				+    ",EMAIL=?"
				+    ",PHONE=?"
				+    ",ADDRESS=? "
				+    "WHERE USERID=?</entry>
<entry key="deleteMember">DELETE FROM MEMBER WHERE USERID =?</entry>
</properties>
